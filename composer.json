{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-dompdf": "^0.8.2",
        "czproject/pdf-rotate": "^2.0",
        "digitaldream/laracrud": "^4.0",
        "fideloper/proxy": "^4.0",
        "iio/libmergepdf": "3.1",
        "laravel/framework": "5.6.*",
        "laravel/scout": "^5.0",
        "laravel/tinker": "^1.0",
        "lynx39/lara-pdf-merger": "1.0.*",
        "petehouston/laravel-tinymce-simple-imageupload": "^1.1",
        "phpmailer/phpmailer": "^6.0",
        "phpoffice/phpspreadsheet": "^1.3",
        "phpoffice/phpword": "v0.14.*",
        "rfookune/lara-pdf-merger-php-7-2": "dev-master",
        "teamtnt/laravel-scout-tntsearch-driver": "^3.0",
        "usmanhalalit/laracsv": "1.*@dev",
      "ext-intl": "*"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Cemas\\": "cemas/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
